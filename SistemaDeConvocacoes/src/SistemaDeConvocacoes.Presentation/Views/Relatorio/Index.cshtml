@{
    Layout = "~/Views/Shared/Processos/LayoutPainelIndex.cshtml";
    ViewBag.Title = "Relatórios";
}
<div class="col-lg-12">
    <div class="row">
        <div class="col-lg-12">
            <h1 class="page-header"><b>@ViewBag.Title</b></h1>
        </div>
    </div>
    <div class="row">
        <div class="col-lg-12">
            <div class="panel-body">
                <div class="container">
                    <div class="form-horizontal">
                        <h3>@Html.ActionLink("Classificados Por Cargo.", "ClassificadosPorCargo", "Relatorio", new { Id = ViewBag.ProcessoId }, null) </h3>
                        <h3>@Html.ActionLink("Convocados que Desistiram.", "ConvocadosQueDesistiram", "Relatorio", new { Id = ViewBag.ProcessoId }, null) </h3>
                        <h3>@Html.ActionLink("Convocados que ingressaram no sistema.", "ConvocadosQueIngressaram", "Relatorio", new { Id = ViewBag.ProcessoId }, null) </h3>
                        <h3>@Html.ActionLink("Convocados que não ingressaram no sistema.", "ConvocadosQueNaoIngressaram", "Relatorio", new { Id = ViewBag.ProcessoId }, null) </h3>
                        <h3>@Html.ActionLink("Convocados por Instituição de Ensino Superior (IES).", "ConvocadosPorInstituicaoEnsinoSuperior", "Relatorio", new { Id = ViewBag.ProcessoId }, null) </h3>
                        <h3>@Html.ActionLink("Relatório de Pendentes de Dados Bancários.", "PendentesDadosBancarios", "Relatorio", new { Id = ViewBag.ProcessoId }, null) </h3>
                        @*<h3>Estatística Geral da Convocação/Contratação.</h3>*@
                    </div>
                </div>
                <div>
                    <button type="button" class="btn btn-secondary" title="" data-placement="left" onclick="history.back();">Voltar</button>
                </div>
            </div>
        </div>
    </div>
</div>