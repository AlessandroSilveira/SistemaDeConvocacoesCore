@{ Layout = "~/Views/Shared/Processos/Configuracoes/_LayoutProcessosConfiguracoes.cshtml";
                ViewBag.Title = "Configurações"; }

<div class="col-lg-12">   
    <div class="card shadow mb-4">
        <div class="card-header py-3">
            <h6 class="m-0 font-weight-bold text-primary">@ViewBag.dadosProcesso.Nome - @ViewBag.Title</h6>
        </div>
        <div class="row">
            <div class="col-lg-12">
                <div class="panel-body">
                    <div class="card-body">
                        <div class="table-responsive">
                            <div class="table-responsive">
                                <div class="table-responsive">
                                    <div>
                                        Nesta página o usuário pode alterar detalhes da convocação como seus atributos, cargos, candidatos e documentação.
                                    </div>
                                </div>
                                <div class="table-responsive">
                                    <div>
                                        <br><br>
                                    </div>
                                </div>
                                <div class="table-responsive">
                                    <div>
                                        <button type="button" class="btn btn-secondary" title="" data-placement="left" onclick="history.back();">Voltar</button>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>