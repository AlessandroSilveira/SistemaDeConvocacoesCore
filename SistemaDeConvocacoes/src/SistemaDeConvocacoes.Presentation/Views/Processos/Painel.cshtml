@{ ViewBag.Title = "Painel"; }
@{ Layout = "~/Views/Shared/Processos/LayoutProcessosPainel.cshtml"; }
<div class="col-lg-12">
    <div class="card shadow mb-4">
        <div class="card-header py-3">
            <h6 class="m-0 font-weight-bold text-primary">@ViewBag.Title</h6>
        </div>
        <div class="row">
            <div class="col-lg-12">
                <div class="panel-body">
                    <div class="card-body">
                        <div class="table-responsive">
                            <div class="col-lg-12">
                                <div class="row">
                                    <div class="col-lg-12">
                                        <h1 class="page-header"><b>@ViewBag.dadosProcesso.Nome</b> </h1>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>