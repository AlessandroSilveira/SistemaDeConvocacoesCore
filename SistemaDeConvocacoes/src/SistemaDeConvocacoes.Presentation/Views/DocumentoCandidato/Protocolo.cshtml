@{
    Layout = "~/Views/Shared/Convocacao/DocumentacaoConvocado/_LayoutConvocacaoDocumentacaoConvocado.cshtml";
    ViewBag.Title = "Protocolo de envio de documento";
}

<div class="col-lg-12">
    <div class="row">
        <div class="col-lg-12">
            <h1 class="page-header"><b>@ViewBag.Title</b></h1>
        </div>
    </div>
    <div class="row">
        <div class="col-lg-12">           
            <div class="panel-body">
                <div class="table-responsive">
                    <h1 text-align="center">Protocolo N° @ViewBag.dadosDocumentoCandidato.DocumentoCandidatoId</h1>
                    <br />
                    <br />
                    <h2>
                        Prezado(a) Candidato(a) @ViewBag.dadosConvocado.Nome.
                    </h2>
                    <br />
                    <br />
                    <h3>
                        Sua documentação foi recebida em @ViewBag.dadosDocumentoCandidato.DataInclusao
                    </h3>
                    <br />
                    <br />
                    <h3>
                        Aguarde as próximas etapas do processo.
                    </h3>
                    <br />
                    <br />
                    <p><input type="button" value="Imprimir" onclick="window.print();" class=" btn btn-primary"  />
                    <input type="button" value="Voltar" onclick="window.history.back()" class=" btn btn-default" /></p>
                </div>
            </div>
        </div>
    </div>
</div>